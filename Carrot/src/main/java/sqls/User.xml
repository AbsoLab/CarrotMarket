<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
        
<mapper namespace="User">

<select id="userInfo" parameterType="Integer" resultType="mul.camp.a.dto.UserDto">
	SELECT *
	FROM USER
	WHERE UID = #{uid}
</select>

<update id="updateUserInfo" parameterType="mul.camp.a.dto.UserDto">
	UPDATE USER
	SET 
	EMAIL = #{email},
	LOCATION = #{location},
	PHONE = #{phone},
	WHERE UID = #{uid}
</update>


</mapper>